create or replace function amt(n in number) 
return number is
aa number;
s number;
r number;
begin
aa:=n;
s:=0;
while aa>0
	loop
	r:=aa mod 10;
	s:=s+r*r*r;
	aa:=trunc(aa/10);
	end loop;
return s;
end;
/

declare 
n number:=&n;
s number;
begin
s:=amt(n);
if s=n
	then
	dbms_output.put_line('Its a armstrong number!!');
else
	dbms_output.put_line('Its not a armstrong number!!');
end if;
end;
/